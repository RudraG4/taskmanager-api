{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "camelcase": 1,
        "curly": 1,
        "no-unused-vars": 1,
        "block-scoped-var": 1
    }
}
